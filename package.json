{
  "name": "liga-a-start-template",
  "version": "1.0.0",
  "description": "Стартовый шаблон сборки Лиги А",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "10.0.2",
    "browser-sync": "2.26.x",
    "eslint": "7.13.0",
    "eslint-config-htmlacademy": "0.6.0",
    "gulp": "4.0.x",
    "gulp-csso": "4.0.1",
    "gulp-imagemin": "7.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0.0",
    "gulp-posthtml": "3.0.5",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "postcss": "8.1.8",
    "posthtml-include": "1.6.0",
    "stylelint": "13.8.0",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "12.18.4"
  },
  "dependencies": {
    "del": "^6.0.0"
  }
}
